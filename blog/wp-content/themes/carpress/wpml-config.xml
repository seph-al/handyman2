<wpml-config>
    <custom-fields>
        <custom-field action="translate">subtitle</custom-field>
        <custom-field action="ignore">slide_template</custom-field>
        <custom-field action="translate">team_custom_fields</custom-field>
        <custom-field action="translate">revo_slider_alias</custom-field>
        <custom-field action="ignore">title_bg</custom-field>
        <custom-field action="ignore">show_front_page</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">services</custom-type>
        <custom-type translate="1">team</custom-type>
        <custom-type translate="1">gallery</custom-type>
        <custom-type translate="1">slider</custom-type>
        <custom-type translate="1">testimonials</custom-type>
    </custom-types>
